[sub_resource type="GDScript" id="GDScript_y5wl6"]
script/source = "extends State

@onready var seed_chooser := $\"../../HUD/SeedChooser\" as SeedChooser
@onready var seed_bank := $\"../../HUD/SeedBank\" as SeedBank
@onready var animation := $\"../../AnimationPlayer\" as AnimationPlayer

func enter() -> void:
    print(\"Loding start...\")
    seed_chooser.init()
    seed_bank.init(GameManager.levels[(owner as GameLevel).level_name][\"initial_sun\"])
    animation.play(\"level_loading\")
    super.enter()

func update(_delta: float) -> void:
    pass
    
func exit() -> void:
    print(\"Loading end.\")
    super.exit()

func _on_animation_player_animation_finished(anim_name: StringName) -> void:
    if not is_active():
        return
    if anim_name == \"level_loading\":
        emit_signal(\"finished\", \"Selecting\")
"

[sub_resource type="GDScript" id="GDScript_6jpss"]
script/source = "extends State

var seed_chooser_event := SeedChooserEvent.new()

func _ready() -> void:
    seed_chooser_event.on_event.connect(_on_event_seed_chooser)
    EventManager.add_listener(seed_chooser_event)

func enter() -> void:
    print(\"Selecting start...\")
    ($\"../../BgmSelecting\" as AudioStreamPlayer).play()
    super.enter()

func update(_delta: float) -> void:
    pass
    
func exit() -> void:
    super.exit()

func _on_event_seed_chooser(e: SeedChooserEvent) -> void:
    if not is_active():
        return
    if e.type == SeedChooserEvent.Type.SELECTION_COMPLETE:
        emit_signal(\"finished\", \"SelectionComplete\")
        
func _exit_tree() -> void:
    EventManager.remove_listener(seed_chooser_event)

"

[sub_resource type="GDScript" id="GDScript_ub4hr"]
script/source = "extends State

@onready var animation := $\"../../AnimationPlayer\" as AnimationPlayer

func enter() -> void:
    print(\"Selection Complete\")
    animation.play(\"selection_complete\")

    var tween := create_tween()
    tween.tween_property(($\"../../BgmSelecting\" as AudioStreamPlayer), \"volume_db\", -80.0, 5)
    
    super.enter()

func update(_delta: float) -> void:
    pass
    
func exit() -> void:
    super.exit()

func _on_animation_player_animation_finished(anim_name: StringName) -> void:
    if not is_active():
        return
    if anim_name == \"selection_complete\":
        emit_signal(\"finished\", \"SetReadyPlant\")
"

